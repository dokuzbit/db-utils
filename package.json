{
  "name": "@dokuzbit/utils",
  "description": "Utility functions for web apps, both server and client.",
  "version": "0.2.6",
  "author": "dokuzbit",
  "license": "MIT",
  "type": "module",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/dokuzbit/utils.git"
  },
  "homepage": "https://github.com/dokuzbit/utils",
  "bugs": {
    "url": "https://github.com/dokuzbit/utils/issues"
  },
  "exports": {
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./server/*": {
      "import": "./dist/server/*.server.js",
      "types": "./dist/server/*.server.d.ts"
    },
    "./client/*": {
      "import": "./dist/client/*.js",
      "types": "./dist/client/*.d.ts"
    }
  },
  "scripts": {
    "test": "bun test",
    "build": "tsc",
    "release": "bun run build && bun publish --access public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "^1.2.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/lodash-es": "^4.17.12",
    "@types/memjs": "^1.3.3",
    "playwright": "^1.51.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.4.0",
    "memjs": "^1.3.2",
    "nats": "^2.29.2"
  }
}
